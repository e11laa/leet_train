#include <bits/stdc++.h>
using namespace std;
 
int main() {
  int A, B;
  cin >> A >> B;  
  cout << "A:";
  while(A > 0){
    A--;
    cout << "]";
    }
             cout << endl;
    cout << "B:";
  while(B > 0){
    B--;
    cout << "]";
             }
  cout << endl;
      
      
 
  // ここにプログラムを追記
}
